<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:filedialog="http://schemas.android.com/apk/res-auto" >

    <com.radicalninja.radfiledialog.FontIcon
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:text="@string/dialog_icon_placeholder"
        android:id="@+id/item_icon"
        filedialog:icon_name="warning-sign"
        android:padding="10dp"
        android:gravity="center"
        android:textSize="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:text="@string/dialog_label_placeholder"
        android:id="@+id/item_label"
        android:padding="10dp"
        android:gravity="center"
        android:textSize="12dp" />

</merge>
